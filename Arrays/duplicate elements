#include <bits/stdc++.h>

using namespace std;

int main()
{
    int arr[]={1,3,4,2,2};
    int n=5;
    sort(arr,arr+n);
    int i;
    for(i=0;i<n-1;++i)
    {
        if(arr[i]==arr[i+1])
        {
            cout<<"duplicate element="<<arr[i];
            break;
        }
    }
    
    if(i==n)
    {
        cout<<"No duplicate element";
        }
    

    return 0;
}
